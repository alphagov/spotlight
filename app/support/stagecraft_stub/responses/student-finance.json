          {
            "label": "Submitted - 2014/15",
            "categoryId": "submitted",
            "comparison": "academic_year:2014/15",
            "format": "integer"
          },
          {
            "label": "Started - 2013/14",
            "categoryId": "started",
            "comparison": "academic_year:2013/14",
            "format": "integer",
