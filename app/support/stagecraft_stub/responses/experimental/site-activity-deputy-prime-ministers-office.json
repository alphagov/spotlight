      "value-attribute": "pageviews:sum",
      "show-line-labels": true,
      "one-hundred-percent": true,
      "use_stack": true,
      "info": [
        "Data source: Google Analytics",
        "This shows the number and percentage of entrances made by three categories of device - desktop, tablet and mobile. The metrics are weekly visits for the last 9 full weeks (Monday to Sunday).",
        "A department’s content is defined as the pages that a department publishes. The metrics only include the content in the departments and policy section of the site. These metrics may be based on sampled data."
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": ["_start_at", "_end_at"],
          "format": {
            "type": "dateRange",
            "format": "D MMM YYYY"
          }
        },
        "y": [
          {
            "label": "Desktop",
            "categoryId": "desktop",
            "format": "integer"
          },
          {
            "label": "Mobile",
            "categoryId": "mobile",
            "format": "integer"
          },
          {
            "label": "Tablet",
            "categoryId": "tablet",
