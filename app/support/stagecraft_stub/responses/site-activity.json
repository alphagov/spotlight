{
  "slug": "site-activity",
  "page-type": "dashboard",
  "dashboard-type": "content",
  "published": true,
  "strapline": "Performance",
  "title": "Activity on GOV.UK",
  "department": {
    "title": "Cabinet Office",
    "abbr": "CO"
  },
  "tagline": "Web traffic on <a href=\"https://www.gov.uk\">our site</a>, based on data from Google Analytics.",
  "modules": [
    {
      "slug": "live-service-usage",
      "module-type": "realtime",
      "title": "Live service usage",
      "description": "Number of users currently on GOV.UK",
      "info": [
        "Data source: Google Analytics",
        "Shows the estimated number of users currently accessing GOV.UK."
      ],
      "data-source": {
        "data-group": "government",
        "data-type": "realtime",
        "query-params": {
          "sort_by": "_timestamp:descending",
          "limit": 722
        }
      }
    },
    {
      "slug": "site-traffic",
      "module-type": "grouped_timeseries",
      "title": "Site traffic",
      "description": "Unique visitors to GOV.UK over the past year, compared with the previous year",
      "category": "website",
      "axis-period": "month",
      "value-attribute": "visitors:sum",
      "show-line-labels": true,
      "use_stack": false,
      "info": [
        "Data source: Google Analytics"
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": {
            "type": "dateRange",
            "format": "D MMM YYYY"
          }
        },
        "y": [
          {
            "label": "GOV.UK",
            "groupId": "govuk",
            "format": "integer"
          },
          {
            "label": "GOV.UK",
            "groupId": "govuk",
            "timeshift": 52,
            "format": "integer"
          },
          {
            "label": "Directgov",
            "groupId": "directgov",
            "timeshift": 52,
            "format": "integer"
          },
          {
            "label": "Business Link",
            "groupId": "businesslink",
            "timeshift": 52,
            "format": "integer"
          }
        ]
      },
      "data-source": {
        "data-group": "govuk",
        "data-type": "visitors",
        "query-params": {
          "period": "week",
          "duration": 52,
          "group_by": "website",
          "collect": "visitors:sum"
        }
      }
    },
    {
      "slug": "top-content",
      "module-type": "list",
      "title": "Top content",
      "description": "Most pageviews in past 7 days",
      "info": [
        "Data source: Google Analytics",
        "The content on GOV.UK with the most pageviews in the past 7 days."
      ],
      "classes": [
        "cols2"
      ],
      "label-attr": "pageTitle",
      "label-regex": "^(.*)\\s-[^-]+$",
      "link-attr": "pagePath",
      "data-source": {
        "data-group": "govuk",
        "data-type": "most_viewed",
        "query-params": {
          "sort_by": "pageviews:descending",
          "limit": 10
        }
      }
    },
    {
      "slug": "trending",
      "module-type": "list",
      "title": "Trending content",
      "description": "Largest percentage increase in pageviews week-on-week",
      "info": [
        "Data source: Google Analytics",
        "Based on percentage increase between total pageviews in past 7 days and total pageviews in previous 7 days. Low pageviews in either period are rounded up to a set threshold.",
        "Excludes search results, help pages, the Service Design Manual, the Performance Platform, policies, and transformation projects."
      ],
      "classes": [
        "cols2"
      ],
      "label-attr": "pageTitle",
      "label-regex": "^(.*)\\s-[^-]+$",
      "link-attr": "pagePath",
      "data-source": {
        "data-group": "govuk",
        "data-type": "trending",
        "query-params": {
          "sort_by": "percent_change:descending",
          "limit": 10
        }
      }
    },
    {
      "slug": "top-policies",
      "module-type": "list",
      "title": "Top policies",
      "description": "Most pageviews in past 7 days",
      "info": [
        "Data source: Google Analytics",
        "The policies on GOV.UK with the most pageviews in the past 7 days."
      ],
      "classes": [
        "cols2"
      ],
      "label-attr": "pageTitle",
      "label-regex": "^(.*?)(?:\\s-\\s(?:Policy\\s-\\s|Policies\\s-\\s)?GOV.UK)?$",
      "link-attr": "pagePath",
      "data-source": {
        "data-group": "govuk",
        "data-type": "most_viewed_policies",
        "query-params": {
          "sort_by": "pageviews:descending",
          "limit": 10
        }
      }
    },
    {
      "slug": "top-announcements",
      "module-type": "list",
      "title": "Top announcements",
      "description": "Most pageviews in past 7 days",
      "info": [
        "Data source: Google Analytics",
        "The press releases and news on GOV.UK with the most pageviews in the past 7 days."
      ],
      "classes": [
        "cols2"
      ],
      "label-attr": "pageTitle",
      "label-regex": "^(.*?)(?:\\s-\\s(?:News stories\\s-\\s|Press releases\\s-\\s)?GOV.UK)?$",
      "link-attr": "pagePath",
      "data-source": {
        "data-group": "govuk",
        "data-type": "most_viewed_news",
        "query-params": {
          "sort_by": "pageviews:descending",
          "limit": 10
        }
      }
    },
    {
      "slug": "device-type",
      "module-type": "grouped_timeseries",
      "title": "How people access GOV.UK",
      "description": "Breakdown of <span class=\"group0\">desktop</span>, <span class=\"group1\">mobile</span> and <span class=\"group2\">tablet</span> usage on GOV.UK over time",
      "category": "deviceCategory",
      "axis-period": "month",
      "value-attribute": "visitors:sum",
      "show-line-labels": true,
      "one-hundred-percent": true,
      "use_stack": false,
      "info": [
        "Data source: Google Analytics",
        "Desktop category includes traffic from desktops and laptops"
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": {
            "type": "dateRange",
            "format": "D MMM YYYY"
          }
        },
        "y": [
          {
            "label": "Desktop",
            "groupId": "desktop",
            "format": "percent"
          },
          {
            "label": "Mobile",
            "groupId": "mobile",
            "format": "percent"
          },
          {
            "label": "Tablet",
            "groupId": "tablet",
            "format": "percent"
          }
        ]
      },
      "data-source": {
        "data-group": "govuk",
        "data-type": "devices",
        "query-params": {
          "period": "week",
          "duration": 52,
          "group_by": "deviceCategory",
          "collect": [
            "visitors:sum"
          ]
        }
      }
    },
    {
      "slug": "service-availability",
      "module-type": "tab",
      "title": "Service availability",
      "info": [
        "Data source: Pingdom",
        "Page load time measures the time taken for a sample page within the service to load.",
        "<a href='/service-manual/operations/uptime-and-availability' rel='external'>Uptime</a> measures the percentage of time that the service was observed to be available."
      ],
      "tabs": [
        {
          "title": "30 days",
          "slug": "30-days",
          "module-type": "availability",
          "description": "",
          "axes": {
            "x": {
              "label": "Date",
              "key": "_timestamp",
              "format": "date"
            }
          },
          "data-source": {
            "data-group": "govuk",
            "data-type": "monitoring",
            "query-params": {
              "period": "day"
            }
          }
        },
        {
          "title": "24 hours",
          "slug": "24-hours",
          "module-type": "availability",
          "description": "",
          "axes": {
            "x": {
              "label": "Time",
              "key": "_timestamp",
              "format": "time"
            }
          },
          "data-source": {
            "data-group": "govuk",
            "data-type": "monitoring",
            "query-params": {
              "period": "hour"
            }
          }
        }
      ]
    }
  ]
}
