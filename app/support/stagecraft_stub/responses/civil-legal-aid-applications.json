{
  "slug": "civil-legal-aid-applications",
  "page-type": "dashboard",
  "dashboard-type": "transaction",
  "published": false,
  "strapline": "Dashboard",
  "title": "Civil legal aid applications",
  "description": "Text to be provided by Legal Aid Agency.",
  "department": {
    "title": "Ministry of Justice",
    "abbr": "MOJ"
  },
  "agency": {
    "title": "Legal Aid Agency",
    "abbr": "LAA"
  },
  "relatedPages": {
    "transaction": {
      "title": "Legal Aid",
      "url": "https://www.gov.uk/legal-aid"
    },
    "improve-dashboard-message": true
  },
  "customer-type": "Individuals",
  "business-model": "Taxpayers",
  "costs": "Text to be provided by Legal Aid Agency.",
  "other-notes": "Text to be provided by the Legal Aid Agency.",
  "modules": [
      {
      "slug": "transactions-per-year",
      "module-type": "kpi",
      "title": "Transactions per year",
      "classes": "cols3",
      "value-attribute": "number_of_transactions",
      "format": {
        "type": "number",
        "magnitude": true,
        "sigfigs": 3
      },
      "info": [
        "Data source: Ministry of Justice"
      ],
      "data-source": {
        "data-group": "transactional-services",
        "data-type": "summaries",
        "query-params": {
          "filter_by": [
            "service_id:moj-legal-aid-applications-civil-cases",
            "type:seasonally-adjusted"
          ],
          "sort_by": "_timestamp:descending"
        }
      }
    },
    {
      "slug": "application-volumetrics",
      "module-type": "grouped_timeseries",
      "title": "Applications processed per week",
      "description": "<span class=\"group0\">Paper (CIS)</span> and <span class=\"group1\">digital (CCMS)</span> applications over time",
      "category": "channel",
      "value-attribute": "count:sum",
      "show-line-labels": true,
      "use_stack": true,
      "info": [
        "Data source: Ministry of Justice",
        "Text to be provided by Legal Aid Agency."
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": "date"
        },
        "y": [
          {
            "label": "Paper (CIS)",
            "groupId": "paper",
            "format": "integer"
          },
          {
            "label": "Digital (CCMS)",
            "groupId": "digital",
            "format": "integer"
          }
        ]
      },
      "data-source": {
        "data-group": "legal-aid",
        "data-type": "transactions-by-channel",
        "query-params": {
          "period": "week",
          "filter_by": [
            "service:legal-aid-civil-claims",
            "transaction:applications",
            "transaction_state:processed"
          ],
          "start_at": "2014-03-31T00:00:00Z",
          "group_by": "channel",
          "collect": [
            "count:sum"
          ]
        }
      }
    },
    {
      "slug": "digital-takeup",
      "module-type": "completion_rate",
      "title": "How many applications processed are made digitally",
      "description": "Proportion of applications made using the digital service",
      "info": [
        "Data source: Ministry of Justice",
        "<a href='/service-manual/measurement/digital-takeup' rel='external'>Digital take-up</a> measures the percentage of completed applications that are made through a digital channel versus non-digital channels."
      ],
      "denominator-matcher": "^(digital|paper)$",
      "numerator-matcher": "^digital$",
      "matching-attribute": "channel",
      "value-attribute": "count:sum",
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": "date"
        },
        "y": [
          {
            "label": "Digital take-up",
            "key": "completion",
            "format": "percent"
          }
        ]
      },
      "axis-period": "week",
      "data-source": {
        "data-group": "legal-aid",
        "data-type": "transactions-by-channel",
        "query-params": {
          "period": "week",
          "filter_by": [
            "service:legal-aid-civil-claims",
            "transaction:applications"
          ],
          "start_at": "2014-03-31T00:00:00Z",
          "group_by": "channel",
          "collect": [
            "count:sum"
          ]
        }
      }
    },
    {
      "slug": "processing-volumetrics",
      "module-type": "grouped_timeseries",
      "title": "Processed applications outcomes per week",
      "description": "Text to be provided by Legal Aid Agency.",
      "category": "process_stage",
      "value-attribute": "count:sum",
      "show-line-labels": true,
      "use_stack": false,
      "info": [
        "Data source: Ministry of Justice",
        "Shows the number of applications processed, broken down by channel",
        "Digital: applications made online (CCMS)",
        "Paper form: applications made using a paper form (CIS)"
      ],
      "axes": {
        "x": {
          "label": "Date",
          "key": [
            "_start_at",
            "_end_at"
          ],
          "format": "date"
        },
        "y": [
          {
            "label": "Issued",
            "groupId": "issued",
            "format": "integer"
          },
          {
            "label": "Offered",
            "groupId": "offered",
            "format": "integer"
          },
          {
            "label": "Refused",
            "groupId": "refused",
            "format": "integer"
          },
          {
            "label": "Further information",
            "groupId": "further-information",
            "format": "integer"
          }
        ]
      },
      "data-source": {
        "data-group": "legal-aid",
        "data-type": "transactions-by-channel",
        "query-params": {
          "period": "week",
          "filter_by": [
            "service:legal-aid-civil-claims",
            "transaction:applications",
            "transaction_state:processed"
          ],
          "start_at": "2014-03-31T00:00:00Z",
          "group_by": "process_stage",
          "collect": [
            "count:sum"
          ]
        }
      }
    },
    {
      "slug": "application-rejections",
      "module-type": "single_timeseries",
      "title": "Applications rejected per month",
      "description": "Number of applications rejected due to incomplete or incorrect information.",
      "info": [
        "Data source: Ministry of Justice",
        "Shows the number of applications rejected each month due to incorrect or invalid information provided by users"
      ],
      "value-attribute": "count:sum",
      "format-options": {
        "type": "integer"
      },
      "axes": {
              "x": {
                "label": "Date",
                "key": [
                  "_start_at",
                  "_end_at"
                ],
                "format": "date"
              },
              "y": [
                {
                  "label": "Rejections",
                  "key": "count:sum",
                  "format": "integer"
                }
              ]
            },
      "axis-period": "month",
      "data-source": {
        "data-group": "legal-aid",
        "data-type": "rejection-reasons",
        "query-params": {
          "period": "month",
          "filter_by": [
            "service:legal-aid-civil-claims",
            "transaction:applications"
          ],
          "start_at": "2014-03-31T00:00:00Z",
          "collect": [
            "count:sum"
          ]
        }
      }
    },
    {
      "slug": "rejection-reasons",
      "module-type": "table",
      "title": "Rejection reasons",
      "description": "Reasons applications were rejected",
      "axes": {
        "x": {
          "label": "Description",
          "key": "reason_text"
        },
        "y": [
          {
            "label": "Volume last month",
            "key": "count:sum",
            "format": "integer"
          },
          {
            "label": "Percentage of total errors",
            "key": "percentOfTotal(count:sum)",
            "format": "percent"
          }
        ]
      },
      "sort-order": "descending",
      "info": [
        "Data source: Ministry of Justice",
        "specific rejection reasons are grouped into categories and reported monthly.</li>"
      ],
      "sort-by": "count:sum",
      "data-source": {
        "data-group": "legal-aid",
        "data-type": "rejection-reasons",
        "query-params": {
          "filter_by": [
            "service:legal-aid-civil-claims",
            "transaction:applications"
          ],
          "group_by": "reason_text",
          "collect": [
            "count:sum"
          ],
          "period": "month",
          "duration": 1
        }
      }
    },
    {
      "slug": "how-users-access-the-service",
      "title": "How users access the civil legal aid applications service",
      "description": "Which devices and browsers users are accessing the content from",
      "module-type": "tab",
      "tabs": [
        {
          "slug": "devices",
          "module-type": "grouped_timeseries",
          "title": "Devices",
          "description": "<span class=\"group0\">Desktop</span>, <span class=\"group1\">mobile</span> and <span class=\"group2\">tablet</span> usage over the last 9 weeks.",
          "category": "deviceCategory",
          "axis-period": "week",
          "value-attribute": "sessions",
          "show-line-labels": true,
          "one-hundred-percent": false,
          "use_stack": false,
          "axes": {
            "x": {
              "label": "Date",
              "key": [
                "_start_at",
                "_end_at"
              ],
              "format": {
                "type": "dateRange",
                "format": "D MMM YYYY"
              }
            },
            "y": [
              {
                "label": "Desktop",
                "groupId": "desktop",
                "format": "integer"
              },
              {
                "label": "Mobile",
                "groupId": "mobile",
                "format": "integer"
              },
              {
                "label": "Tablet",
                "groupId": "tablet",
                "format": "integer"
              }
            ]
          },
          "data-source": {
            "data-group": "legal-aid-civil-claims",
            "data-type": "device-usage",
            "query-params": {
              "period": "week",
              "duration": 9,
              "group_by": "deviceCategory",
              "collect": [
                "sessions"
              ]
            }
          }
        },
        {
          "slug": "browsers",
          "module-type": "grouped_timeseries",
          "title": "Browsers",
          "category": "browser",
          "axis-period": "week",
          "value-attribute": "sessions",
          "show-line-labels": true,
          "one-hundred-percent": false,
          "use_stack": false,
          "group-mapping": {
            "Chrome:(.)*": "Chrome",
            "Safari:(.)*": "Safari",
            "Internet Explorer:[2-8]{1}(.)*": "OldIE",
            "Internet Explorer:(9|1)(.)*": "NewIE",
            "Firefox:(.)*": "Firefox"
          },
          "axes": {
            "x": {
              "label": "Date",
              "key": [
                "_start_at",
                "_end_at"
              ],
              "format": {
                "type": "dateRange",
                "format": "D MMM YYYY"
              }
            },
            "y": [
              {
                "label": "Google Chrome",
                "groupId": "Chrome",
                "format": "integer"
              },
              {
                "label": "Internet Explorer (IE9+)",
                "groupId": "NewIE",
                "format": "integer"
              },
              {
                "label": "Internet Explorer (pre IE9)",
                "groupId": "OldIE",
                "format": "integer"
              },
              {
                "label": "Safari",
                "groupId": "Safari",
                "format": "integer"
              },
              {
                "label": "Firefox",
                "groupId": "Firefox",
                "format": "integer"
              },
              {
                "label": "Other",
                "groupId": "other",
                "format": "integer"
              }
            ]
          },
          "data-source": {
            "data-group": "legal-aid-civil-claims",
            "data-type": "browser-usage",
            "query-params": {
              "period": "week",
              "duration": 9,
              "group_by": [
                "browser",
                "browserVersion"
              ],
              "collect": [
                "sessions"
              ]
            }
          }
        }
      ]
    }
  ]
}
